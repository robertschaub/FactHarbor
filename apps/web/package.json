{
  "name": "@factharbor/web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --webpack -p 3000",
    "dev:turbo": "next dev -p 3000",
    "build": "next build",
    "postbuild": "npx tsx scripts/reseed-all-prompts.ts --quiet",
    "start": "next start -p 3000",
    "reseed": "npx tsx scripts/reseed-all-prompts.ts",
    "reseed:force": "npx tsx scripts/reseed-all-prompts.ts --force",
    "reseed:prompts": "npx tsx scripts/reseed-all-prompts.ts --prompts",
    "reseed:configs": "npx tsx scripts/reseed-all-prompts.ts --configs",
    "test": "vitest run",
    "test:llm": "vitest run test/unit/lib/llm-integration.test.ts",
    "test:neutrality": "vitest run test/unit/lib/input-neutrality.test.ts",
    "test:expensive": "vitest run test/unit/lib/llm-integration.test.ts test/unit/lib/input-neutrality.test.ts test/integration/claimboundary-integration.test.ts",
    "test:cb-integration": "vitest run test/integration/claimboundary-integration.test.ts",
    "test:calibration": "vitest run test/calibration/political-bias.test.ts -t \"quick mode\"",
    "test:calibration:quick": "vitest run test/calibration/political-bias.test.ts -t \"quick mode\"",
    "test:calibration:full": "vitest run test/calibration/political-bias.test.ts -t \"full mode\"",
    "test:jobs": "vitest run test/unit/lib/job-lifecycle.test.ts",
    "test:baseline": "node scripts/baseline-runner.js",
    "promptfoo": "promptfoo eval --env-file .env.local",
    "promptfoo:sr": "promptfoo eval -c promptfooconfig.source-reliability.yaml --env-file .env.local",
    "promptfoo:view": "promptfoo view"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.39",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/openai": "^3.0.1",
    "ai": "^6.0.78",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "kill-port": "^2.0.1",
    "next": "^16.1.6",
    "pdf2json": "^4.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.6.0",
    "react-json-view-lite": "^2.5.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "promptfoo": "^0.120.23",
    "typescript": "^5.5.4",
    "vitest": "^4.0.18"
  }
}
